    
#include <kipr/botball.h>

int main()
{
   motor(1,75);
   motor(2,75);
    msleep(4000);
    motor(1,300);
    msleep(2500);
    motor(1,100);
    motor(2,100);
    msleep(5000);
    motor(1,800);
    msleep(4000);
    motor(1,75);
    motor(2,75);
    msleep(3500);
    motor(2,300);
    msleep(2500);
    motor(1,100);
    motor(2,100);
    msleep(11000);
    int counter = 1700;
    set_servo_position(0, counter);
    set_servo_position(1,2047);
    enable_servos();
    while (counter > 380)
    {
        msleep(1000);
        counter = counter - 30;
        set_servo_position(0, counter);
    }
    msleep(1000);
    disable_servos();
    return 0;
}
